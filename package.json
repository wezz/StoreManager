{
  "name": "@wezz/store-manager",
  "version": "0.1.4",
  "description": "A script to manage local and session storage",
  "type": "module",
  "main": "./dist/storemanager.umd.cjs",
  "module": "./dist/storemanager.js",
  "types": "./dist/index.d.ts",
  "exports": {
    ".": {
      "import": "./dist/storemanager.js",
      "require": "./dist/storemanager.umd.cjs"
    }
  },
  "files": [
    "dist"
  ],
  "scripts": {
    "dev": "vite",
    "build": "tsc && vite build",
    "prepare": "npm run build",
    "pack": "npm run build",
    "trypublish": "npm publish --access public || true"
  },
  "repository": {
    "type": "git",
    "url": "https://github.com/wezz/StoreManager"
  },
  "author": "Wezz Balk <github@wezz.se> (https://github.com/wezz)",
  "license": "MIT",
  "bugs": {
    "url": "https://github.com/wezz/StoreManager/issues"
  },
  "homepage": "https://github.com/wezz/StoreManager",
  "keywords": [
    "helper",
    "localstorage",
    "sessionstorage",
    "window"
  ],
  "devDependencies": {
    "@types/node": "^18.14.2",
    "typescript": "^4.9.3",
    "vite": "^4.1.0",
    "vite-plugin-dts": "^2.0.2"
  }
}
